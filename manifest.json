{
  "manifest_version": 3,
  "name": "Google Messages Web Multi-Delete",
  "version": "1.0",
  "permissions": [ "activeTab" ],
  "host_permissions": [ "https://messages.google.com/*" ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
}

